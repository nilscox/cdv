{
  "name": "e2e",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "cypress open",
    "start:local": "docker-compose -f docker-compose.yml -f docker-compose.local.yml build && docker-compose -f docker-compose.yml -f docker-compose.local.yml up --abort-on-container-exit",
    "start:open": "docker-compose -f docker-compose.yml -f docker-compose.open.yml up --abort-on-container-exit",
    "start:open-local": "docker-compose -f docker-compose.yml -f docker-compose.open.yml -f docker-compose.local.yml up --abort-on-container-exit",
    "start:ci": "docker-compose up --abort-on-container-exit"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@types/node": "^12.7.12",
    "cypress": "^3.4.1",
    "cypress-cucumber-preprocessor": "^1.16.1",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
