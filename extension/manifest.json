{
  "manifest_version": 2,
  "version": "0.6.3",
  "name": "Zétécom (dev)",
  "description": "L'information sur internet, avec esprit critique",
  "browser_specific_settings": {
    "gecko": {
      "id": "",
      "strict_min_version": "65.0"
    }
  },
  "permissions": ["background"],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ]
}
