{
  "manifest_version": 2,
  "version": "0.5.1",
  "name": "Réagir à l'information",
  "description": "Réagir à l'information sur internet, avec esprit critique !",
  "browser_specific_settings": {
    "gecko": {
      "id": "{89e0d6c7-e0ea-470c-88cf-0d22c8ef27eb}",
      "strict_min_version": "65.0"
    }
  },
  "permissions": [
    "background"
  ],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["integrations/youtube.js"]
    },
    {
      "matches": ["*://*.skeptikon.fr/*"],
      "js": ["integrations/skeptikon.js"]
    },
    {
      "matches": ["*://*.lemonde.fr/*"],
      "js": ["integrations/lemonde.js"]
    }
  ]
}
