{
  "compilerOptions": {
    "target": "es6",
    "moduleResolution": "node",
    "isolatedModules": false,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "sourceMap": true
  },
  "include": [
    "src",
  ]
}
