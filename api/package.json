{
  "name": "zetecom-api",
  "description": "Zétécom - API",
  "version": "0.4.2",
  "license": "GPL-3.0-only",
  "author": "nilscox <contact@zetecom.fr>",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && cp -r src/modules/email/templates dist/src/modules/email",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:coverage": "nyc --silent yarn start",
    "start:dev": "NODE_ENV=development nodemon",
    "start:debug": "NODE_ENV=development nodemon --config nodemon-debug.json",
    "start:prod": "yarn db:migrate && node dist/src/main.js",
    "lint": "eslint --ext .ts src/**",
    "test": "NODE_ENV=test jest",
    "test:watch": "yarn test --watch",
    "test:cov": "yarn test --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:intg": "yarn test --config ./test/jest-intg.json --runInBand",
    "test:e2e": "yarn test --config ./jest-e2e.config.js --runInBand",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "start:restoredb": "ep2cmd /reset 'yarn db:reset' /populate 'yarn db:populate'",
    "db:drop": "yarn typeorm schema:drop",
    "db:migrate": "yarn typeorm migration:run",
    "db:populate": "ts-node ./scripts/populate",
    "db:reset": "yarn db:drop && yarn db:migrate",
    "db:dump": "yarn db:drop && ../scripts/dump-database.sh",
    "deploy": "../scripts/deploy-api.sh"
  },
  "dependencies": {
    "@nest-middlewares/cors": "^6.0.0",
    "@nest-middlewares/express-session": "^6.0.0",
    "@nest-middlewares/morgan": "^6.0.0",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/typeorm": "^6.1.1",
    "@types/node": "^10.12.18",
    "bcrypt": "^3.0.6",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "dotenv": "^8.0.0",
    "emailjs": "^2.2.0",
    "express-session": "^1.16.1",
    "handlebars": "^4.7.6",
    "memorystore": "^1.6.1",
    "mjml": "^4.6.3",
    "module-alias": "^2.2.0",
    "pg": "^7.10.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "typeorm": "^0.2.25",
    "typescript": "^3.2.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@nestjs/testing": "^6.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^25.2.3",
    "@types/mjml": "^4.0.4",
    "@types/module-alias": "^2.0.0",
    "@types/multer": "^1.4.3",
    "@types/supertest": "^2.0.7",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "ep2cmd": "https://github.com/nilscox/ep2cmd.git",
    "eslint": "^6.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-nibble": "^5.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "express": "^4.17.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "supertest": "^3.4.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "wait-on": "^4.0.0"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true
  }
}
